{
  "name": "we-connet",
  "version": "1.0.0",
  "description": "An application that connects businesses and individuals",
  "main": "app.js",
  "scripts": {
    "test-migrate": "sequelize db:migrate:undo:all --env test && sequelize db:migrate --env test",
    "test": "sequelize db:migrate:undo:all --env test && sequelize db:migrate --env test && nyc --reporter=html --reporter=text mocha ./server/test/ --exit --timeout 10000 --compilers js:babel-register ",
    "clean": "rimraf dist",
    "build": "npm run clean && mkdir dist && babel server -s -d dist",
    "start": "npm run build && babel-watch dist/app.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GodwinEkuma/we-connect.git"
  },
  "author": "Godwin Ekuma",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GodwinEkuma/we-connect/issues"
  },
  "homepage": "https://github.com/GodwinEkuma/we-connect#readme",
  "dependencies": {
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "express": "^4.16.2",
    "joi": "^13.1.2",
    "jsonwebtoken": "^8.2.0",
    "mocha": "^5.0.4",
    "morgan": "^1.9.0",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.36.0",
    "swagger-ui-express": "^2.0.15",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-watch": "^2.0.7",
    "coveralls": "^3.0.0",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.4.1"
  }
}
